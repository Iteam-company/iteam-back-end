{
  "name": "iteam",
  "version": "0.0.1",
  "description": "Iteam",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "12 || 14 || 16 || 17"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "pretest": "npm run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js",
    "docker:build": "docker build -t iteam .",
    "docker:run": "docker run -p 3000:3000 -d iteam",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run rebuild",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build && npm run init-uploads",
    "postinstall": "npm run build",
    "init-uploads": "cd dist && npx mkdirp public/uploads",
    "start:prod": "npm run init-uploads && node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Igor <sergienko339@gmail.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^8.0.0",
    "@loopback/authentication-jwt": "^0.10.0",
    "@loopback/boot": "^4.0.0",
    "@loopback/core": "^3.0.0",
    "@loopback/repository": "^4.0.0",
    "@loopback/rest": "^11.0.0",
    "@loopback/rest-explorer": "^4.0.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.1",
    "cloudinary": "^1.28.1",
    "dotenv": "^11.0.0",
    "jsonwebtoken": "^8.5.1",
    "loopback-connector-postgresql": "^5.4.0",
    "mkdirp": "^1.0.4",
    "multer": "1.4.3",
    "passport-google-oauth": "^2.0.0",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@loopback/build": "^8.0.0",
    "@loopback/testlab": "^4.0.0",
    "@types/node": "^10.17.60",
    "@types/uuid": "^8.3.4",
    "source-map-support": "^0.5.20",
    "typescript": "~4.5.2"
  }
}
