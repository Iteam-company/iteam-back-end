{
	"name": "iteam",
	"version": "0.0.1",
	"description": "Iteam",
	"keywords": [
		"loopback-application",
		"loopback"
	],
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"engines": {
		"yarn": "1.22.19",
		"node": "16.17.0"
	},
	"scripts": {
		"prestart": "yarn rebuild",
		"rebuild": "yarn clean && yarn build && yarn init-uploads",
		"start": "node dist/index.js",
		"dev": "node -r source-map-support/register .",
		"clean": "lb-clean dist *.tsbuildinfo .eslintcache",
		"build": "lb-tsc",
		"build:watch": "lb-tsc --watch",
		"pretest": "yarn rebuild",
		"postinstall": "yarn build",
		"test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
		"test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js",
		"docker:build": "docker build -t iteam .",
		"docker:run": "docker run -p 3000:3000 -d iteam",
		"preopenapi-spec": "yarn build",
		"openapi-spec": "node ./dist/openapi-spec",
		"init-uploads": "cd dist && npx mkdirp public && npx mkdirp public/uploads",
		"start:prod": "yarn init-uploads && node dist/index.js",
		"lint": "yarn run eslint src",
		"prepare": "husky install"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/iteam-comp/iteam-back-end"
	},
	"author": "Igor <sergienko339@gmail.com>, Yevhen <jekasemenuk@icloud.com>",
	"license": "MIT",
	"files": [
		"README.md",
		"dist",
		"src",
		"!*/__tests__"
	],
	"dependencies": {
		"@loopback/authentication": "^8.0.0",
		"@loopback/authentication-jwt": "^0.10.0",
		"@loopback/boot": "^4.0.0",
		"@loopback/core": "^3.0.0",
		"@loopback/repository": "^4.0.0",
		"@loopback/rest": "^11.0.0",
		"@loopback/rest-explorer": "^4.0.0",
		"bcrypt": "^5.0.1",
		"body-parser": "^1.19.1",
		"cloudinary": "^1.28.1",
		"dotenv": "^11.0.0",
		"jsonwebtoken": "^8.5.1",
		"loopback-connector-postgresql": "^5.4.0",
		"mkdirp": "^1.0.4",
		"multer": "1.4.3",
		"passport-google-oauth": "^2.0.0",
		"tslib": "^2.0.0",
		"uuid": "^8.3.2",
		"@loopback/build": "^8.0.0",
		"@loopback/testlab": "^4.0.0",
		"@types/bcrypt": "^5.0.0",
		"@types/multer": "^1.4.7",
		"@types/node": "^10.17.60",
		"@types/uuid": "^8.3.4",
		"@typescript-eslint/eslint-plugin": "^5.38.1",
		"@typescript-eslint/parser": "^5.38.1",
		"eslint": "^8.0.1",
		"eslint-config-prettier": "^8.5.0",
		"eslint-config-standard-with-typescript": "^23.0.0",
		"eslint-plugin-import": "^2.25.2",
		"eslint-plugin-n": "^15.0.0",
		"eslint-plugin-promise": "^6.0.0",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"prettier": "2.7.1",
		"source-map-support": "^0.5.20",
		"typescript": "*"
	},
	"devDependencies": {},
	"lint-staged": {
		"**/*": "prettier --write --ignore-unknown"
	}
}
